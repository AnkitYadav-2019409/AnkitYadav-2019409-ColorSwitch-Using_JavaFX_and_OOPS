<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<fx:root minHeight="600.0" minWidth="600.0" type="AnchorPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="MenuController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@bg.jpg" />
         </image>
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <Button fx:id="SavedGameButton" layoutX="140.0" layoutY="130.0" mnemonicParsing="false" onMouseClicked="#SavedGameController" prefHeight="55.0" prefWidth="62.0" text="Saved Games" textAlignment="CENTER" textOverrun="CLIP" wrapText="true" />
      <Button fx:id="settings" layoutX="140.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#SettingController" prefHeight="55.0" prefWidth="62.0" text="Settings" textAlignment="CENTER" textOverrun="CLIP" wrapText="true" />
      <Button fx:id="help" layoutX="402.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#HelpController" prefHeight="55.0" prefWidth="62.0" text="Help" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="PlayButton" layoutX="261.0" layoutY="213.0" mnemonicParsing="false" onMouseClicked="#PlayController" prefHeight="84.0" prefWidth="92.0" text="Play" textAlignment="CENTER" wrapText="true" />
   </children>
</fx:root>
